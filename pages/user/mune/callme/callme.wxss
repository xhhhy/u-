.content{
  width: 90%;
  margin: 40px auto;
  }
  .content image{
    width: 100%;
    height: 200px;
  }
  .online image{
    width: 50px;
    height: 50px
  }
  .online{
      padding: 30px 0px 30px 20px;
      text-align: center;
      border: 4px solid #f6f5f5;
      border-radius: 20px;
      font-weight: 500;
      margin-top: 30px;
  }
  .col-classa{
    text-align: left;
    padding-left: 20px;
    line-height: 1.5;
  }
  .onlinefont{
    font-size: 14px;
    color: #7f7e7e;
  }
  .col-classb{
    line-height: 40px;
  }
.footer{
  border: 4px solid #f6f5f5;
  margin-top: 30px;
  border-radius: 20px;
  font-weight: 500;
  text-align: center;
  line-height: 2;
  padding: 40px 0 40px 0;
}
.footer image{
  width: 150px;
  height: 150px
}